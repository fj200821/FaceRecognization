    <string name="app_name">crypt</string>
    <string name="u_cancel">Cancel</string>
    <string name="u_confirm">Update</string>
    <string name="u_content">A new version is available, download now?</string>
    <string name="u_current_progress">Current progress:</string>
    <string name="u_download_complete">Download completed, click it for installation</string>
    <string name="u_downloading">Downloading</string>
